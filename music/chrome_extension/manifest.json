{
    "manifest_version": 3,
    "name": "Random Click and Scroll",
    "version": "1.2",
    "description": "Scroll and click automation",
    "permissions": ["scripting", "activeTab"],
    "host_permissions": ["<all_urls>"],
    "action": {
      "default_popup": "popup.html",
      "default_icon": "icon.png"
    },
    "background": {
      "service_worker": "background.js"
    }
  }
  